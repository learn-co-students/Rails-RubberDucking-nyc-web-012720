<% if flash[:errors] %>%
    <% flash[:errors].each do |error| %>
    <%= error %>
    <% end %>
<% end %>%

<%= form_for @duck do |f| %>
<p> Duck Name: <%= f.text_field :name %>
<p> Duck Description: <%= f.text_field :description %>
<p> Duck Owner: <%= f.collection_select :student_id, Student.all, :id, :name %>
<%= f.submit %>
<% end %>