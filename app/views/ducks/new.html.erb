<% if flash[:errors] %> 
    <% flash[:errors].each do |error| %>
        <p><%= error %></p>
    <% end %>
<% end %>
<%= form_for @duck do |f| %>
    <p>Duck Name: <%= f.text_field :name %></p>
    <p>Duck Description: <%= f.text_field :description %></p>
    <p>Owner: <%= f.collection_select :student_id, Student.all, :id, :name %></p>
    <%= f.submit "Add this Duck Fam"%>
<% end %>